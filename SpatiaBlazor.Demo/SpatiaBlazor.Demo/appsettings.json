{
  "DetailedErrors": true,
  "Logging": {
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information"
      }
    },
    "ApplicationInsights": {
      "IncludeScopes": false,
      "LogLevel": {
        "Default": "Warning",
        "System": "Error",
        "Microsoft": "Error"
      }
    },
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Error",
      "Microsoft.Hosting.Lifetime": "Error",
      "System": "Error",
      "Microsoft.AspNetCore.SignalR": "Warning"
    }
  },
  "AllowedHosts": "*"
}
